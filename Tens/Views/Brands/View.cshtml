@model Tens.Models.brand
@{
    ViewBag.Title = "View";
    Layout = "~/Views/Shared/_Layout.cshtml";
}


<div class="container-fluid">
	<div class="col-lg-12 col-md-12 col-sm-12 col-sx-12">
		<div class="panel-body">
			<div class="row">
				<div class="pull-right">
					<a href="@Url.Content("~/Brands")" data-toggle="tooltip" title="Back To Grid" class="btn btn-default"><i class="fa fa-refresh"></i> Back</a>
				</div>
			</div>
		</div>
	</div>
	<div class="col-lg-12 col-md-12 col-sm-12 col-sx-12">
		<div class="panel panel-default">
			<div class="panel-heading">
				<h3 class="panel-title">Brands</h3>
			</div>
			<div class="panel-body">
				@using (Html.BeginForm()) 
                { 
                    <div class="form-group required">
						<label class="col-sm-2 control-label">Brands Name</label>
						<div class="col-sm-6">
							 @Html.LabelForModel(Model.brand_name)
						</div>
					</div>
					
                }
			</div>
		</div>
	</div>
</div>

